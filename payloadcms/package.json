{
  "name": "ethnicast-cms",
  "version": "1.0.0",
  "description": "This is a [Next.js](https://nextjs.org/) project bootstrapped with [`create-next-app`](https://github.com/vercel/next.js/tree/canary/packages/create-next-app).",
  "main": "index.js",
  "scripts": {
    "client:dev": "cd ../client && npm run dev",
    "payload:dev": "NODE_ENV=development PAYLOAD_CONFIG_PATH=src/payload.config.ts nodemon",
    "start:dev": "concurrently 'npm run payload:dev' 'npm run client:dev'",
    "build:server": "tsc --project tsconfig.server.json",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload build",
    "build": "cross-env NODE_ENV=production npm run build:payload && npm run build:server && npm run build:next",
    "serve": "cross-env NODE_ENV=production PAYLOAD_CONFIG_PATH=dist/payload.config.js node dist/server.js",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
    "generate:graphql-schema": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:graphqlSchema",
    "payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@faceless-ui/modal": "^2.0.1",
    "@payloadcms/bundler-webpack": "^1.0.0",
    "@payloadcms/db-mongodb": "^1.0.0",
    "@payloadcms/plugin-form-builder": "^1.0.15",
    "@payloadcms/plugin-search": "^1.1.0",
    "@payloadcms/richtext-slate": "^1.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "escape-html": "^1.0.3",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "nodemailer-sendgrid": "^1.0.3",
    "payload": "^2.0.0",
    "punycode": "^2.3.1",
    "react-hook-form": "^7.48.2",
    "react-select": "^5.8.0",
    "rss-parser": "^3.13.0",
    "swr": "^2.2.4"
  },
  "devDependencies": {
    "@babel/register": "^7.22.15",
    "@hookform/devtools": "^4.3.1",
    "@swc/core": "^1.3.99",
    "@trbl/eslint-config": "^3.0.1",
    "@types/express": "^4.17.9",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.46",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "concurrently": "^8.2.2",
    "copyfiles": "^2.4.1",
    "nodemon": "^2.0.22",
    "payload-plugin-form-builder": "^1.0.8",
    "sass": "^1.69.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.8.4"
  }
}
